@model IEnumerable< WebApplication1.Models.tblProduct>

@{
    ViewBag.Title = "Index";
}

<h2>Products List</h2>
<ol>
    @foreach (var item in Model)
    {
        <li>@Html.ActionLink(item.name,"Details",new { id=item.productid})  </li>
    }
</ol>
<br/>
@Html.ActionLink("Back to categories ","Index","Categories")

